CREATE DATABASE medicaid_db;
USE medicaid_db;
    drop table if exists PERSON;
   create table PERSON (
        FIRST_NAME varchar(255) not null,
        LAST_NAME varchar(255),
        SSN varchar(255),
        DATE_OF_BIRTH datetime,
        ADDRESS varchar(255),
        PHONE_NUMBER varchar(255),
        EMAIL varchar(255),
        INCOME varchar(255),
        ISEMPLOYED bit,
        HASDEPENDENTS bit,
        primary key (FIRST_NAME)
    )
